<div class="container">
    <div class="feature-container">
        {% for item in items['cards'] %}
            <div class="feature-card">
                <img src="{{item.img}}" alt="{{item.header}}">
                <h4>{{item.header}}</h4>
                <p>{{item.body}}</p>  
            </div>
        {% endfor %}
    </div>
</div>
